{
  "manifest_version": 3,
  "name": "Ijudge Capture Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
  "activeTab",
  "scripting",
  "downloads"
  ],
  "background": {
    "service_worker": "./script/background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "ijudge_blueberry.png"
  }
}
